{
  "author": "afairhurst",
  "license": "ISC",
  "name": "js-aprs-engine",
  "version": "1.0.0",
  "homepage": "https://github.com/KD0NKS/js-aprs-engine",
  "description": "Engine for bundling libraries and settings to run an APRS application.",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KD0NKS/js-aprs-engine.git"
  },
  "bugs": {
    "url": "https://github.com/KD0NKS/js-aprs-engine/issues"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src/**/*.ts",
      "typings/**/*.d.ts"
    ]
  },
  "keywords": [
    "APRS",
    "ham radio",
    "amateur radio"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "tsc": "./node_modules/.bin/tsc",
    "setup": "npm install",
    "build": "webpack",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.0",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "file-loader": "^5.0.2",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "js-aprs-fap": "^1.0.0",
    "js-aprs-is": "^1.0.1"
  }
}
